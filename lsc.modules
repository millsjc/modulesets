<?xml version="1.0"?><!--*- mode: nxml; indent-tabs-mode: nil -*-->
<!DOCTYPE moduleset SYSTEM "moduleset.dtd">
<?xml-stylesheet type="text/xsl" href="moduleset.xsl"?>
<!-- vim:set ts=2 expandtab: -->
<moduleset>
  <repository type="git" name="git.ligo.org"
      href="https://git.ligo.org/lscsoft/" default="yes"/>
  <repository type="git" name="ligo-cbc"
      href="https://github.com/ligo-cbc/"/>
  <repository type="tarball" name="software.ligo.org"
      href="http://software.ligo.org/lscsoft/source/"/>
  <repository type="svn" name="svn.ligo.caltech.edu"
      href="https://galaxy.ligo.caltech.edu/svn/"/>

  <distutils id="ligo-proposal-tool">
    <branch repo="git.ligo.org" module="ligo-proposal-tool"/>
  </distutils>

  <!-- FIXME: lalsuite autotools packages do not yet support non-srcdir builds
  under JHBuild; see https://bugs.ligo.org/redmine/issues/644
  and https://bugzilla.gnome.org/show_bug.cgi?id=689472 -->

  <autotools id="lal" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lal"/>
  </autotools>

  <autotools id="lalframe" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lalframe"/>
    <dependencies>
      <dep package="lal"/>
    </dependencies>
  </autotools>

  <autotools id="lalmetaio" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lalmetaio"/>
    <dependencies>
      <dep package="lal"/>
    </dependencies>
  </autotools>

  <autotools id="lalxml" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lalxml"/>
    <dependencies>
      <dep package="lal"/>
    </dependencies>
  </autotools>

  <autotools id="lalinspiral" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lalinspiral"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
    </dependencies>
  </autotools>

  <autotools id="lalburst" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lalburst"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
    </dependencies>
  </autotools>

  <autotools id="lalsimulation" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python --enable-openmp">
    <branch module="lalsuite" subdir="lalsimulation"/>
    <dependencies>
      <dep package="lal"/>
    </dependencies>
  </autotools>

  <autotools id="lalpulsar" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python --enable-openmp --enable-cfitsio">
    <branch module="lalsuite" subdir="lalpulsar"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalxml"/>
    </dependencies>
  </autotools>

  <autotools id="lalstochastic" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="lalstochastic"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalmetaio"/>
    </dependencies>
  </autotools>

  <autotools id="lalinference" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '"  autogenargs="--enable-swig-python --enable-openmp">
    <branch module="lalsuite" subdir="lalinference"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalxml"/>
      <dep package="lalsimulation"/>
      <dep package="lalinspiral"/>
      <dep package="lalburst"/>
      <dep package="lalpulsar"/>
      <dep package="ligo-segments"/>
      <dep package="skyarea"/>
      <dep package="corner"/>
    </dependencies>
  </autotools>

  <autotools id="laldetchar" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '"  autogenargs="--enable-swig-python">
    <branch module="lalsuite" subdir="laldetchar"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
      <dep package="lalburst"/>
    </dependencies>
  </autotools>

  <autotools id="lalapps" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00boot &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--enable-openmp --enable-mpi --enable-cfitsio">
    <branch module="lalsuite" subdir="lalapps"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalxml"/>
      <dep package="lalsimulation"/>
      <dep package="lalburst"/>
      <dep package="lalinspiral"/>
      <dep package="lalpulsar"/>
      <dep package="lalinference"/>
      <dep package="lalstochastic"/>
      <dep package="glue"/>
    </dependencies>
  </autotools>

  <autotools id="lalsuite-extra">
    <branch repo="software.ligo.org" module="lalsuite-extra-1.3.0.tar.gz" version="1.3.0" hash="sha256:c3341af345169592af115134183222be3e22e209c4c292f9ccbb1b69cddf33a2" />
  </autotools>

  <distutils id="pylal">
    <branch module="pylal"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
      <dep package="lalburst"/>
      <dep package="lalinspiral"/>
    </dependencies>
  </distutils>

  <distutils id="glue">
    <branch module="glue"/>
  </distutils>

  <distutils id="ligo-segments">
    <branch module="ligo-segments"/>
    <dependencies>
      <dep package="ligo-common"/>
    </dependencies>
  </distutils>

  <distutils id="ligo-common">
    <branch module="ligo-common"/>
  </distutils>

  <distutils id="ligo.skymap">
    <branch module="leo-singer/ligo.skymap"/>
  </distutils>

  <distutils id="gwcelery">
    <branch module="emfollow/gwcelery"/>
  </distutils>

  <distutils id="lvalert">
    <branch repo="git.ligo.org" module="lvalert.git"/>
    <dependencies>
      <dep package="ligo-common"/>
    </dependencies>
  </distutils>

  <distutils id="gracedb">
    <branch repo="git.ligo.org" module="gracedb-client.git"/>
    <dependencies>
      <dep package="glue"/>
      <dep package="ligo-common"/>
    </dependencies>
  </distutils>

  <distutils id="grinch">
    <branch module="grinch"/>
    <dependencies>
      <dep package="gracedb"/>
      <dep package="lvalert"/>
    </dependencies>
  </distutils>

  <metamodule id="lalsuite">
    <dependencies>
      <dep package="lal"/>
      <dep package="laldetchar"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalxml"/>
      <dep package="lalburst"/>
      <dep package="lalpulsar"/>
      <dep package="lalstochastic"/>
      <dep package="lalinspiral"/>
      <dep package="lalsimulation"/>
      <dep package="lalinference"/>
      <dep package="lalapps"/>
      <dep package="lalsuite-extra"/>
      <dep package="glue"/>
      <dep package="pylal"/>
      <dep package="gracedb"/>
      <dep package="lvalert"/>
      <dep package="bayeswave"/>
    </dependencies>
  </metamodule>

  <autotools id="gstlal" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00init.sh &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--without-doxygen">
    <branch module="gstlal" subdir="gstlal"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
      <dep package="lalburst"/>
      <dep package="lalinspiral"/>
      <dep package="pylal"/>
      <dep package="glue"/>
    </dependencies>
  </autotools>

  <autotools id="gstlal-ugly" autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00init.sh &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--without-doxygen">
    <branch module="gstlal" subdir="gstlal-ugly"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
      <dep package="lalburst"/>
      <dep package="lalinspiral"/>
      <dep package="pylal"/>
      <dep package="glue"/>
      <dep package="gstlal"/>
    </dependencies>
  </autotools>

  <autotools id="gstlal-inspiral"  autogen-template="bash -c 'pushd %(srcdir)s &amp;&amp; ./00init.sh &amp;&amp; popd &amp;&amp; %(srcdir)s/configure --prefix %(prefix)s %(autogenargs)s '" autogenargs="--without-doxygen">
    <branch module="gstlal" subdir="gstlal-inspiral"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalsimulation"/>
      <dep package="lalburst"/>
      <dep package="lalinspiral"/>
      <dep package="pylal"/>
      <dep package="glue"/>
      <dep package="gstlal"/>
    </dependencies>
  </autotools>

  <metamodule id="gstlal-all">
  	<dependencies>
  	  <dep package="gstlal"/>
  	  <dep package="gstlal-ugly"/>
  	  <dep package="gstlal-inspiral"/>
  	</dependencies>
  </metamodule>

  <distutils id="pycbc-glue">
    <branch repo="ligo-cbc" module="pycbc-glue"/>
  </distutils>

  <distutils id="pycbc">
    <branch repo="ligo-cbc" module="pycbc"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalsimulation"/>
      <dep package="pycbc-glue"/>
      <dep package="pylal"/>
      <dep package="kombine"/>
    </dependencies>
  </distutils>

  <autotools id="bayeswave" supports-non-srcdir-builds="no" skip-autogen="true" skip-install="true" check-target="false" makeargs="--directory=src">
    <branch repo="svn.ligo.caltech.edu" module="bayeswave" tag="O2_offline"/>
    <dependencies>
      <dep package="lal"/>
      <dep package="lalframe"/>
      <dep package="lalmetaio"/>
      <dep package="lalpulsar"/>
      <dep package="lalinspiral"/>
      <dep package="lalsimulation"/>
      <dep package="lalinference"/>
    </dependencies>
  </autotools>

  <autotools id="libframe">
    <branch repo="software.ligo.org" module="libframe-8.30.tar.gz" version="8.30"
      hash="sha256:374743ccce835427a0900590a515c112661760e12ec596159d3f86073dd099da"/>
  </autotools>

  <autotools id="metaio">
    <branch repo="software.ligo.org" module="metaio-8.4.0.tar.gz" version="8.4.0"
      hash="sha256:8990d5aa23614328880d15af72071009a578cedf75174e437b3d63643b850b76"/>
  </autotools>
</moduleset>
